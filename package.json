{
  "name": "voicepartnerai-platform",
  "version": "2.0.0",
  "description": "Enterprise Voice AI Platform for the German Market - Vapi Alternative",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "build": "tsc && npm run build:client",
    "build:client": "webpack --mode production",
    "test": "jest",
    "test:voice": "jest --config jest.voice.config.js",
    "lint": "eslint src/**/*.ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "docker:build": "docker build -t voicepartnerai .",
    "docker:run": "docker run -p 8080:8080 voicepartnerai"
  },
  "dependencies": {
    "@google-cloud/speech": "^6.2.0",
    "@google-cloud/text-to-speech": "^5.0.0",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.20.1",
    "rate-limiter-flexible": "^2.4.2",
    "redis": "^4.6.10",
    "sip.js": "^0.21.2",
    "socket.io": "^4.7.4",
    "twilio": "^4.19.0",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.13",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.1",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "voice-ai",
    "german-market",
    "enterprise",
    "speech-to-text",
    "text-to-speech",
    "workflow-automation",
    "assistant-management",
    "real-time-communication"
  ],
  "author": "VoicePartnerAI Team",
  "license": "UNLICENSED",
  "private": true
}
